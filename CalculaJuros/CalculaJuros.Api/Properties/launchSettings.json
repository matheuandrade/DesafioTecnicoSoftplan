{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:50708",
      "sslPort": 44352
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/calculajuros",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "CalculaJuros.Api": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/calculajuros",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "TAXA_JUROS_API": "https://localhost:5001"
      },
      "applicationUrl": "https://localhost:4001;http://localhost:4000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/calculajuros",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}